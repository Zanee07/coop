{
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/threads",
      "destination": "/api/threads.js"
    },
    {
      "source": "/api/messages",
      "destination": "/api/messages.js"
    },
    {
      "source": "/api/runs",
      "destination": "/api/runs.js"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    }
  ]
}
